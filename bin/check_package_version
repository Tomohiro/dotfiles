#!/bin/bash

packages=(
    zsh vim emacs screen irssi ruby gem rvm perl python lv sshfs
    keychain xsel brew java php gcc make fortune git subersion colordiff
    w3m wget curl
)


for p in ${packages[@]}; do
    if [ `which $p` ]; then
        $p --version > /dev/null 2>&1
        if [ $? = 0 ]; then
            version="`$p --version | sed '/^ *$/d' | head -n 1`"
        else
            version="`$p -v | sed '/^ *$/d' | head -n 1`"
        fi
    else
        version="$p is not installed"
    fi
    echo $version
done
