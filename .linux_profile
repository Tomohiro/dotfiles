# keychain settings
HOST=`hostname`
KEYCHAIN=`which keychain`
IDENTITY=~/.ssh/id_rsa
SSH_AGENT=~/.keychain/$HOST-sh
$KEYCHAIN $IDENTITY
 . $SSH_AGENT

# User specific environment and startup programs
PATH=$HOME/bin:$PATH:/sbin:/usr/sbin
export PATH

if [ -f ~/.network ]; then
    . ~/.network
fi

case $TERM in
    linux) LANG=C ;;
    *) LANG=ja_JP.UTF-8 ;;
esac

# Change shell ( bash to zsh )
exec `which zsh`
unset USERNAME
